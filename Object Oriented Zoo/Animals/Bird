// Subclass 1: Bird
class Bird extends Animal implements Flyable {
    public Bird(String name) {
        super(name);
    }

    @Override
    public void makeSound() {
        System.out.println(name + " makes a honking sound.");
    }

    @Override
    public void move() {
        System.out.println(name + " can waddel, swim, and fly.");
    }

    @Override
    public void fly() {
        System.out.println(name + " flies in the sky.");
    }

    @Override
    public void swim() {
        System.out.println(name + " swims on water.");
    }
}