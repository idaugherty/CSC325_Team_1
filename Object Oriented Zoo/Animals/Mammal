// Subclass 3: Mammal
class Mammal extends Animal implements Runnable {
    public Mammal(String name) {
        super(name);
    }

    @Override
    public void makeSound() {
        System.out.println(name + " makes a growling sound.");
    }

    @Override
    public void move() {
        System.out.println(name + " can run on land.");
    }
}